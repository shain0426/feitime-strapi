{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "pid": {
      "type": "uid",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "english_name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "origin": {
      "type": "string",
      "required": true
    },
    "region": {
      "type": "string",
      "required": false
    },
    "processing": {
      "type": "string",
      "required": true
    },
    "roast": {
      "type": "string",
      "required": true
    },
    "flavor_type": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Fruity",
        "Floral",
        "Nutty",
        "Bold"
      ]
    },
    "flavor_tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::flavor.flavor"
    },
    "acidity": {
      "type": "integer",
      "required": true,
      "min": 0,
      "max": 5
    },
    "sweetness": {
      "type": "integer",
      "required": true,
      "min": 0,
      "max": 5
    },
    "body": {
      "type": "integer",
      "required": true,
      "min": 0,
      "max": 5
    },
    "aftertaste": {
      "type": "integer",
      "required": true,
      "min": 0,
      "max": 5
    },
    "clarity": {
      "type": "integer",
      "required": true,
      "min": 0,
      "max": 5
    },
    "description": {
      "type": "text",
      "required": true
    },
    "popularity": {
      "type": "integer",
      "required": true,
      "min": 0,
      "max": 5
    },
    "price": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "stock": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "weight": {
      "type": "string",
      "default": "250g",
      "required": true
    },
    "img": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "recommend_grind": {
      "type": "decimal"
    },
    "recommend_temp": {
      "type": "integer"
    },
    "recommend_ratio": {
      "type": "decimal"
    },
    "simulation_preset": {
      "type": "json"
    },
    "cart_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cart-item.cart-item",
      "mappedBy": "product"
    },
    "brew_logs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::brew-log.brew-log",
      "mappedBy": "product"
    }
  }
}
